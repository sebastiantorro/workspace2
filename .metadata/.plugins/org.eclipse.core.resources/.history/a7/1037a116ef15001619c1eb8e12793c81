package cl.curso.java.control;

public class Libro {

String nombreLibro;
String autorLibro;
int nEjemplares;
int nEjemplaresP;

 public Libro (){
this.nombre=nombreLibro;
this.autor=autorLibro;
this.numerosejemplares=nEjemplares;
this.numerosejemplaresP=nEjemplaresP;

	
}
 public Libro (String nombre,String autor, int numerosejemplares, int numerosejemplaresp)
 {
	 this.nombre=nombreLibro;
	 this.autor=autorLibro;
	 this.numerosejemplares=nEjemplares;
	 this.numerosejemplaresP=nEjemplaresP;

	
}
